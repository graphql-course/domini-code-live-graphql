    "@types/compression": "1.7.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.26",
    "@types/mongodb": "^3.5.25",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript-tslint-plugin": "0.5.5"
  }
